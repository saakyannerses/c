#include <stdio.h>

int main()
{
   int arr[7];
   printf("Введите 7 целых чисел:\n");
   
   for(int i = 0; i < 7; i++){
       printf("Элемент %d:", i);
       scanf("%d", &arr[i]);
   }
   
   printf("\nМассив заполнен следующими значениями:\n");
   
   for(int i = 0; i < 7; i++){
       printf("%d", arr[i]);
   } 
   
   printf("\n");
   
    return 0;
}
