#include <stdio.h>
#include <math.h>

int main() {
    float x, y, z, q;

    // Ввод значений x и y
    printf("Введите значение x: ");
    scanf("%f", &x);
    printf("Введите значение y: ");
    scanf("%f", &y);

    // Вычисление значения z
    z = (x + (2 + y) / (x * x)) / (y + 1 / sqrt(x * x + 10));

    // Вычисление значения q
    q = 7.2 * sin(x) - fabs(y);

    printf("Значение z: %f\n", z);
    printf("Значение q: %f\n", q);

    return 0;
}
