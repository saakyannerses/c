#include <stdio.h>
#include <math.h>

int main() {
    float x, y;

    // Ввод значения x
    printf("Введите значение x: ");
    scanf("%f", &x);

    // Вычисление значения y
    if (x > 0) {
        y = pow(sin(x), 2);
    } else {
        y = 1 - 2 * pow(sin(x), 2);
    }

    printf("Значение y: %f\n", y);

    return 0;
}
